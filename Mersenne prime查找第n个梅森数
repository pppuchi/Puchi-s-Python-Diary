from math import sqrt
my_number=int(input("which one:"))

#找素数
def isprime(x):
    if x==1:
        return False
    k=int(sqrt(x))
    for j in range(2,k+1):
        if x%j==0:
            return False
    return True

#创建两个列表存放素数
Mersenne_list=[]
Prime_list=[]

#嵌套结构，一直循环直到找到第n个梅森数
p=1
while (len(Mersenne_list)<my_number):
    p+=1
    if isprime(p):
        Prime_list.append(p)
        m=2**p-1
        if isprime(m):
            Mersenne_list.append(m)
                
print(Prime_list)
print(Mersenne_list) 
