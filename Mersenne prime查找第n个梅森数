from math import sqrt
my_number3=int(input("which one:"))

def isprime(x):
    if x==1:
        return False
    k=int(sqrt(x))
    for j in range(2,k+1):
        if x%j==0:
            return False
    return True

monisen_list3=[]
prime_list3=[]


p=1
while (len(monisen_list3)<my_number3):
    p+=1
    if isprime(p):
        prime_list3.append(p)
        m=2**p-1
        if isprime(m):
            monisen_list3.append(m)
                
print(prime_list3)
print(monisen_list3) 
